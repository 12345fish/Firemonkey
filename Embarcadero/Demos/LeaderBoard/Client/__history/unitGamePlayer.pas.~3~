unit unitGamePlayer;

interface

type
  TGamePlayer = class
  private
    FName: string;
    FScore: Integer;
  public
    constructor Create(aName : string);
    property Name : string read FName write FName;
    property Score : Integer read FScore write FScore;
  end;

implementation

{ TGamePlayer }

constructor TGamePlayer.Create(aName: string);
begin
  inherited Create;
  Name := aName;
end;

end.
